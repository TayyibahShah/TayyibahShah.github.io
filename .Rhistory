# Data
yeardeath <- c(2022, 2022, 2022, 2022, 2022, 2021, 2021, 2021, 2021, 2021,
2020, 2020, 2020, 2020, 2020, 2019, 2019, 2019, 2019, 2019,
2018, 2018, 2018, 2018, 2018)
femalesdeaths <- c(46, 46, 25, 32, 33, 23.9, 24.7, 15.1, 12.8, 7.8,
25.6, 18.2, 13.1, 12.1, 6.5, 22.6, 16.1, 7, 7.5, 4.5,
